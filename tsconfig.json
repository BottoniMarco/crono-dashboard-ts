{
  "compilerOptions": {
    "jsx": "react-jsx",            
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2020",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "isolatedModules": true,
    "baseUrl": "./",
    "paths": { "@/*": ["src/*"] },
    "types": ["vite/client", "react", "react-dom"] 
  },
  "include": ["src"]
}
